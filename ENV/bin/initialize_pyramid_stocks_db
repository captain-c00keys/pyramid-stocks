#!/Users/genepieterson/codefellows/401/labs-python/pyramid-stocks/ENV/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pyramid-stocks','console_scripts','initialize_pyramid_stocks_db'
__requires__ = 'pyramid-stocks'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyramid-stocks', 'console_scripts', 'initialize_pyramid_stocks_db')()
    )
